<h1>Figures</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% @figures.each do |figure| %>
  <tr>
    <td><%= figure.name %></td>
    <td><a href= "/figures/<%=figure.id%>">Show</a></td>
    <td><a href= "/figures/<%=figure.id%>/edit">Edit</a></td>
  </tr>
  <% end %>
</table>
<br>
